@page
@model BackendLab01.Pages.Quiz.QuizzesModel
@{
    ViewData["Title"] = "Quizzes";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

@foreach (var quiz in Model.Quizzes)
{
    <a class="nav-link text-dark" asp-page="/Quiz/Item" asp-route-itemId="1" asp-route-quizId="@quiz.Id">@quiz.Title</a>
    <br />
}